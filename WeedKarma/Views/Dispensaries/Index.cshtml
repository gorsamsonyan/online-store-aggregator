<section class="store-filters-section">
    <div class="store-filters-mobile-button">
        Filters
        <i class="fas fa-chevron-down"></i>
    </div>
    <div class="container--medium store-filters">
        <div class="row store-filters-row">

            <div class="col-lg-5 col-md-8 col-sm-12">
                <div class="row">
                    <div class="s col-lg-6">
                        <section class="switching-block st-filter-switch">
                            <div class="fx-fx-center-v switching-item ">
                                <img src="~/img/store/filters/open-now-icon.png" alt="">
                                <input type="checkbox" id="switchingCheckbox__1" class="switchingCheckbox" checked />
                                <label class="fx fx-center-v" for="switchingCheckbox__1">dispensaries</label>
                            </div>
                            <div class="fx-fx-center-v switching-item ">
                                <img src="~/img/store/filters/delivery-package-icon.png" alt="">
                                <input type="checkbox" id="switchingCheckbox__2" class="switchingCheckbox" checked />
                                <label class="fx fx-center-v" for="switchingCheckbox__2">DELIVERIES</label>
                            </div>
                            <div class="fx-fx-center-v switching-item ">
                                <img src="~/img/store/filters/near-me-icon.png" alt="">
                                <input type="checkbox" id="switchingCheckbox__3" class="switchingCheckbox" />
                                <label class="fx fx-center-v" for="switchingCheckbox__3">Near Me</label>
                            </div>
                        </section>
                    </div>

                    <div class=" col-lg-6">
                        <section class="switching-block st-filter-switch">
                            <div class="fx-fx-center-v switching-item ">
                                <img src="~/img/store/filters/gift-card-icon.png" alt="">
                                <input type="checkbox" id="switchingCheckbox__4" class="switchingCheckbox" />
                                <label class="fx fx-center-v" for="switchingCheckbox__4">Gift Cart Pay</label>
                            </div>
                            <div class="fx-fx-center-v switching-item ">
                                <img src="~/img/store/filters/buy-point-icon.png" alt="">
                                <input type="checkbox" id="switchingCheckbox__5" class="switchingCheckbox" />
                                <label class="fx fx-center-v" for="switchingCheckbox__5">Pay with POINTS</label>
                            </div>
                            <div class="fx-fx-center-v switching-item ">
                                <img src="~/img/store/filters/open-now-icon.png" alt="">
                                <input type="checkbox" id="switchingCheckbox__6" class="switchingCheckbox" checked />
                                <label class="fx fx-center-v" for="switchingCheckbox__6">Open Now</label>
                            </div>
                        </section>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-12">
                        <section class="st-filter-search store-filter-item">

                            <div class="">
                                <input type="search" class="filter-search-input"
                                       placeholder="for dispensaries, deliveries & brands">
                            </div>
                        </section>
                    </div>
                </div>
            </div>

            <div class="col-lg-6 col-md-8 col-sm-12">
                <div class="row">
                    <div class=" col-lg-6 flex-align-between">

                        <section class="st-filter-map store-filter-item">
                            <h4 class="text-center st-filter-item__title">
                                <span class="switching-map">

                                    <input type="checkbox" id="switchingCheckboxMap" class="switchingCheckbox" />
                                    <label for="switchingCheckboxMap" class="switchingCheckboxMap-text">show on Map</label>
                                </span>
                            </h4>
                            <div class="map-image">

                            </div>

                            <section class="st-filter-district store-filter-item">
                                <!--<h4 class="text-center st-filter-item__title">Select your district</h4>-->
                                <div class="wkOptions__box1 wkOptions__box-district">
                                    <select class="wkOptions__select1 wkOptions__select" name="skin-type"
                                            data-weedkarma-filter="">
                                        <option value="" selected="" disabled="">Select District</option>
                                        <option value="">District1</option>
                                        <option value="">District2</option>
                                        <option value="">District3</option>
                                        <option value="">District4</option>
                                        <option value="">District5</option>
                                    </select>
                                </div>
                            </section>
                        </section>

                    </div>


                    <div class=" col-lg-6 flex-align-between ">
                        <section class="st-filterRating store-filter-item">
                            <h4 class="text-center st-filter-item__title">Stores Weed High Score</h4>
                            <div class="fx  st-filterRating__container">
                                <form class="st-filterRating__item" method="POST">
                                    <input type="checkbox" class="st-filterRating__checkbox"
                                           id="st-filterRating--1">
                                    <label for="st-filterRating--1" class="st-filterRating__sup">+1</label>
                                </form>
                                <form class="st-filterRating__item" method="POST">
                                    <input type="checkbox" class="st-filterRating__checkbox"
                                           id="st-filterRating--2">
                                    <label for="st-filterRating--2" class="st-filterRating__sup">+2</label>
                                </form>
                                <form class="st-filterRating__item" method="POST">
                                    <input type="checkbox" class="st-filterRating__checkbox"
                                           id="st-filterRating--3">
                                    <label for="st-filterRating--3" class="st-filterRating__sup">+3</label>
                                </form>
                                <form class="st-filterRating__item" method="POST">
                                    <input type="checkbox" class="st-filterRating__checkbox"
                                           id="st-filterRating--4">
                                    <label for="st-filterRating--4" class="st-filterRating__sup">+4</label>
                                </form>
                                <form class="st-filterRating__item" method="POST">
                                    <input type="checkbox" class="st-filterRating__checkbox"
                                           id="st-filterRating--5">
                                    <label for="st-filterRating--5" class="st-filterRating__sup">+5</label>
                                </form>
                            </div>
                        </section>
                        <section class="st-filter-service store-filter-item">
                            <!--<h4 class="text-center st-filter-item__title">Select Service</h4>-->
                            <div class="wkOptions__box1 wkOptions__box-district">
                                <select class="wkOptions__select1 wkOptions__select" name="skin-type"
                                        data-weedkarma-filter="">
                                    <option value="" selected="" disabled="">Select Services</option>
                                    <option value="">service1</option>
                                    <option value="">service1</option>
                                    <option value="">service1</option>
                                    <option value="">service1</option>
                                    <option value="">service1</option>
                                </select>
                            </div>
                        </section>

                    </div>
                    <div class="">

                    </div>

                </div>
            </div>
           
            @*<div class="col-lg-1 range-block">

                <div class="range-slider">
                    <input type="range" value="0" orient="vertical" min="0" max="100" />
                    <div class="range-slider__bar"></div>
                    <div class="range-slider__thumb"> </div>
                    <div class="range-slider-main__thumb"><i class="fas fa-caret-right"></i></div>
                </div>

            </div>*@
        </div>
    </div>

</section>

